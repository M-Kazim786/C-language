#include<stdio.h>
#include<string.h>
typedef struct{
	char name[50];
	int id;
	float gpa;
	
} studentinfo;
int main () {
	studentinfo stdu[3];
	
//	strcpy(stdu[0].name,"Muhammad Kazim");
//	stdu[0].id= 6;
//	stdu[0].gpa=3.0;
	
//	strcpy(stdu[1].name,"Asjal amir");
