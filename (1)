//	stdu[1].gpa=3.5;
	
//	strcpy(stdu[2].name,"Milo Baby");
//	stdu[2].id=8;
//	stdu[2].gpa=3.6;

    printf("Enter name :");
    fgets(stdu[0].name,50,stdin);
    printf("Enter id :");
    scanf("%d",&stdu[0].id);
    printf("Enter gpa");
    scanf("%f",&stdu[0].gpa);
    getchar();
    
    printf("Enter name :");
    fgets(stdu[1].name,50,stdin);
    printf("Enter id :");
    scanf("%d",&stdu[1].id);
    printf("Enter gpa");
    scanf("%f",&stdu[1].gpa);
    getchar();
    
    printf("Enter name :");
    fgets(stdu[2].name,50,stdin);
    printf("Enter id :");
    scanf("%d",&stdu[2].id);
    printf("Enter gpa");
    scanf("%f",&stdu[2].gpa);
    getchar();
    
    printf("\n");
	
	
    for(int i=0;i<3;i++){
    	printf("Name : %s\nstudent id : %d\nstudent gpa : %f\n",stdu[i].name,stdu[i].id,stdu[i].gpa);
	}
	
	
	return 0;
}
