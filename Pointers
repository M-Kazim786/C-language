//print the value of i from its pointer to pointer
#include<stdio.h>
int main() { 
	int i;
	int *ptr= &i;
	int **pptr=&ptr;
	
	printf("Enter value of i :");
	scanf("%d",&i);

	printf("%u",**pptr);	
	return 0;
}
